		{ 
			"matches": [ "http://prohardver.hu/*", "http://mobilarena.hu/*", "http://itcafe.hu/*", "http://logout.hu/*" ], 
			"js": [ "js/ph_1280_fix.js" ] 
		} 
	],
	"manifest_version": 2,
	"version": "1.1.0"
}
