{
	"name": "Custom Home Page",
	"description": "Replace Speed Dial with a home page or a blank page",
	"permissions": [ "tabs", "storage" ],
	"version": "1.0.0",
	"background": 
	{
		"scripts": [ "js/background.js" ]
  	},
  	"icons":
   	{
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"   	
	},
   	"options_page": "html/options.html",
	"web_accessible_resources": [ "html/empty.html" ],
	"manifest_version": 2
}
